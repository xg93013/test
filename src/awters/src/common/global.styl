* {
    padding:0;
    margin:0;
    box-sizing: border-box;
    color white
    // 滚动条样式
    &::-webkit-scrollbar{
        background-color:rgba(20, 20, 37,1);
        width:16px;
    }
    // &::-webkit-scrollbar-track {
    //     background-color:rgba(44, 44, 77,0.1)
    // }
    // &::-webkit-scrollbar-track-piece {
    //     background-color:rgba(44, 44, 77,0.2)
    // }
    &::-webkit-scrollbar-thumb {
        border-radius:3px;
        height: 30px;
        background-color:rgba(44,44,77,0.5);
        &:hover{
            background-color:rgba(44,44,77,1);
            cursor :pointer
        }
    }
    // height 5400px
}
html {
    height:100%;
    width:100%;
    min-height: 1080px;
    min-width 1600px;
    body {
        height:100%;
        width:100%;
    }
}

.floatL {
    float:left;
}

.floatR {
    float:right;
}

.clearFloat
    &:after
        display block
        clear both
        content ""

.tableCommon 
