#app
    .el-loading-mask
        background-color rgba(69, 103, 204, .7)

.el-popover
    background-color rgba(69, 103, 204, .7)
    color white
    border none

// 复选框样式
.el-checkbox{
    .el-checkbox__input{
        &.is-checked{
            .el-checkbox__inner{
                background-color rgba(69, 103, 204,1)
                border-color rgba(69, 103, 204,1)
            }
        }
        &.is-focus{
            .el-checkbox__inner{
                border-color rgba(69, 103, 204,1)
            }
        }
        .el-checkbox__inner{
            background-color transparent
        }
    }
    &.is-checked{
        
        .el-checkbox__label{
            color rgba(69, 103, 204,1)
        }
    }
}

.selectOptionCommon.el-select-dropdown{
    background-color RGBA(27, 27, 38, .9)
    border none
    .el-select-dropdown__wrap{
        .el-select-dropdown__list{
            li{
                trans()
                &.selected{
                    background-color RGBA(57, 89, 166, .3)
                }
                &:hover{
                    background-color RGBA(69, 103, 204, .3)
                }
            }
        }
    }
    .popper__arrow{
        border-bottom-color RGBA(27, 27, 38, .9);
        &:after{
            border-bottom-color RGBA(27, 27, 38, .9);
        }
    }
}
.el-select {
    .el-input{
        .el-input__inner{
            background-color rgba(7, 14, 32,.85)
            // border-color rgba(7, 14, 32,.85)
            border none
            color white
        }
        &.is-focus{
            .el-input__inner{
                background-color rgba(1,156,255,0.85)
            }
        }
    }
    .el-select__tags{
        .el-tag--small{
            background-color rgba(7, 14, 32,.85)
        }
    }
}
.el-select-dropdown.el-popper[x-placement^=bottom]{
    background-color rgba(7, 14, 32,.85)
    border none
    .el-select-dropdown__item{
        &.selected{
            background-color transparent
        }
        &.hover,&:hover,&.hover.selected{
            background-color rgba(1,133,217,.2)
        }
        // &:hover{
        //     background-color transparent
        // }
    }
    .popper__arrow{
        border none
        &:after{
            border-bottom-color rgba(7, 14, 32,.85)
        }
    }
}

.el-collapse {
    background transparent
    border-color rgba(1,156,255,0.5)
    .el-collapse-item {
        .el-collapse-item__header {
            &:not(.is-active) {
                border-color rgba(1,156,255,0.5)
            }
            background transparent
            color white
            clf()
            .prefix,.suffix {
                float left
                width calc((100% - 21px) / 2)
                overflow hidden
                text-overflow ellipsis
                white-space nowrap
            }
            .prefix {
                padding-left 10px
            }
            .suffix {
                text-align right
                padding-right 15px
            }
        }
        .el-collapse-item__wrap {
            background transparent
            border-color rgba(1,156,255,0.5)
            .el-collapse-item__content{
                padding 0
            }
        }
    }
}

.el-progress {
    .el-progress-circle__track{
        stroke transparent
    }
}


@keyframes loadingFloat {
    0% {
        transform translate(-10px,-5px)
    }
    25% {
        transform translate(-5px,5px)
    }
    50% {
        transform translate(0px,-5px)
    }
    75% {
        transform translate(5px,5px)
    }
    100% {
        transform translate(10px,-5px)
    }
